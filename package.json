		"@guardian/prettier": "^2.1.5",
		"@guardian/tsconfig": "^0.2.0",
		"@jest/types": "^29.3.1",
		"@nrwl/esbuild": "15.9.2",
		"@nrwl/eslint-plugin-nx": "15.9.2",
