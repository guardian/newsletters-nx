		"@nrwl/vite": "15.9.2",
		"@nrwl/workspace": "15.9.2",
		"@testing-library/react": "13.4.0",
		"@types/jest": "29.4.0",
		"@types/node": "18.11.9",
		"@types/react": "18.0.25",
		"@types/react-dom": "18.0.9",
		"@types/react-router-dom": "5.3.3",
